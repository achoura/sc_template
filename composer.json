{
    "name": "achoura/sc-template",
    "description": "Project template for Drupal 8 sites built with the Social commerce distribution.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drupal/drupal-extension": "~3.2.0",
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "drupal/coder": "8.*",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "behat/behat": "^3.0",
        "se/selenium-server-standalone": "^2.53",
        "composer/composer": "^1.3",
        "drush/drush": "8.1.11",
        "drupal/console": "^1.0"
    },
    "require": {
        "composer/installers": "^1.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "achoura/sc-profile": "dev-master",
        "malsup/cycle": "3.0.3-a",
        "woocommerce/FlexSlider": "2.6.3",
        "briancherne/jquery-hoverIntent": "dev-master",
        "douglascrockford/JSON-js":"dev-master",
        "mehrpadin/Superfish-for-Drupal":"2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "git@github.com:achoura/sc_profile.git"
        },
        {
          "type": "package",
          "package": {
            "name": "malsup/cycle",
            "version": "3.0.3-a",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/malsup/cycle/archive/master.zip",
              "type": "zip"
            },
            "source": {
              "url": "git@github.com:malsup/cycle.git",
              "type": "git"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "briancherne/jquery-hoverIntent",
            "version": "dev-master",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/briancherne/jquery-hoverIntent/archive/master.zip",
              "type": "zip"
            },
            "source": {
              "url": "git@github.com:briancherne/jquery-hoverIntent.git",
              "type": "git"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "woocommerce/FlexSlider",
            "version": "2.6.3",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/woocommerce/FlexSlider/archive/master.zip",
              "type": "zip"
            },
            "source": {
              "url": "git@github.com:woocommerce/FlexSlider.git",
              "type": "git"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "douglascrockford/JSON-js",
            "version": "dev-master",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/douglascrockford/JSON-js/archive/master.zip",
              "type": "zip"
            },
            "source": {
              "url": "git@github.com:douglascrockford/JSON-js.git",
              "type": "git"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "mehrpadin/Superfish-for-Drupal",
            "version": "2.0",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/mehrpadin/Superfish-for-Drupal/archive/2.0.zip",
              "type": "zip"
            },
            "source": {
              "url": "git@github.com:mehrpadin/Superfish-for-Drupal.git",
              "type": "git"
            }
          }
        }
    ],
    "scripts": {
      "post-install-cmd": [
        "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
      ],
      "post-update-cmd": [
        "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
      ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true
    }
}
